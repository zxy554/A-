#include<stdio.h>
 
int main()
{
    int i,j,s;
    printf("\n");
    for(i=1;i<5;i++) { // 以下为三重循环
        for(j=1;j<5;j++) {
            for (s=1;s<5;s++) { // 确保i、j、k三位互不相同
                if (i!=s&&i!=j&&j!=s) { 
                    printf("%d,%d,%d\n",i,j,k);
                }
            }
        }
    }
}
